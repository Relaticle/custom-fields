includes:

parameters:
    level: 6
    paths:
        - src
        - tests
    excludePaths:
        - tests/Fixtures
        - tests/database
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true

    ignoreErrors:
        # Allow dynamic properties on models for testing
        - '#Access to an undefined property [a-zA-Z0-9\\_]+::\$[a-zA-Z0-9_]+#'